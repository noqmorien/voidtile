// ~/.config/fastfetch/config.jsonc
{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "display": {
        "separator": " \u001b[90m󱕆 ",
        "color": {
            "title": "blue"
        }
    },
    "logo": {
        "source": "~/.config/fastfetch/logo.txt",
        "height": 6,
        "padding": {
            "top": 0,
            "left": 3,
            "right": 12
        }
    },
    "modules": [
        {
            "type": "title",
            "keyWidth": 10
        },
        "break",
        {
            "type": "os",
            "key": "  ",
            "format": "{3}",
            "keyColor": "32"
        },
        {
            "type": "kernel",
            "key": " 󰍛 ",
            "keyColor": "33"
        },
        {
            "type": "wm",
            "key": " 󰭠 ",
            "keyColor": "34"
        },
        {
            "type": "shell",
            "key": "  ",
            "keyColor": "35"
        },
        {
            "type": "terminal",
            "key": "  ",
            "keyColor": "36"
        },
        {
            "type": "uptime",
            "key": " 󰔚 ",
            "keyColor": "37"
        },
        {
            "type": "memory",
            "key": "  ",
            "keyColor": "35"
        },
        {
            "type": "packages",
            "key": " 󰏗 ",
            "keyColor": "34"
        },
        "break",
        {
            "type": "custom",
            "format": "     \u001b[90m󰊠  \u001b[31m󰊠  \u001b[32m󰊠  \u001b[33m󰊠  \u001b[34m󰊠  \u001b[35m󰊠  \u001b[36m󰊠  \u001b[37m󰊠 "
        }
    ]
}
